<header class="row header">
<section class="col-md-8 mt-5 mx-auto p-0">
{% if config.image %}<img class="img-fluid mb-3 img-profile-header" width="100%" height="300" src="https://wsrv.nl/?url={{metadata.url}}{{config.image}}&w=1200&h=300&output=webp" alt="{{title}}"/>{% endif %}
<p class="text-desc text-center lead">{{text}}</p>
<article class="p-3 text-start">{{ content |safe }}</article>
</section>
</header>
